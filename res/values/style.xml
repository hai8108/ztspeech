<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="dialogButton" parent="@android:style/Widget.Button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/top_trans_btn_talk</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="dialog" parent="@android:style/Theme.NoTitleBar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:windowBackground">@color/background_light_black</item>
    </style>
    <style name="listview" parent="@android:style/Widget.TextView">
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:state_focused">false</item>
    </style>
    <style name="lmtitle11" parent="android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#330000</item>
        <item name="android:textSize">22sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/ic_launcher</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:paddingTop">20dip</item>
    </style>
    <style name="center_button" parent="android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/ic_launcher</item>
        <item name="android:textColor">#330000</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingBottom">7dip</item>
        <item name="android:paddingLeft">12dip</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>
    <!-- modified by zengsonghai (shopping model) start -->
    <style name="font_black_middle">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">16px</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="detail_info_text">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">18px</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="font_mauve_middle">
        <item name="android:textColor">#ffcb99</item>
        <item name="android:textSize">20px</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="font_white_middle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">18px</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="font_white_title">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="dialog_button" parent="android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/top_trans_btn_talk</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="click_button" parent="android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/top_trans_btn_talk</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="jumpListPage" parent="android:style/Widget.EditText">
        <item name="android:layout_width">93dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/scrolledit</item>
        <item name="android:textColor">@color/text_color</item>
    </style>
    <style name="SpinnerStyle" parent="@android:style/Widget.Spinner">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">36px</item>
        <item name="android:paddingLeft">5dip</item>
        <item name="android:paddingRight">4dip</item>
        <item name="android:layout_marginTop">4dip</item>
        <item name="android:layout_marginBottom">2dip</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="back_button" parent="android:style/Widget.TextView">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginLeft">7dip</item>
		<item name="android:textSize">15sp</item>
		<item name="android:textColor">@color/button_text</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">center</item>
		<item name="android:background">@drawable/back_button</item>
	</style>
	<style name="bottom_button" parent="android:style/Widget.TextView">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">#000000</item>
		<item name="android:gravity">center</item>
		<item name="android:layout_gravity">center</item>
		<item name="android:layout_marginRight">8dip</item>
		<item name="android:background">@drawable/bottom_button</item>
		<item name="android:textStyle">bold</item>
		<item name="android:padding">6dip</item>
	</style>
	<style name="listitem_s" >
		<item name="android:textStyle">bold</item>
		<item name="android:cacheColorHint">#00000000</item>
		<item name="android:background">@drawable/listitem1</item>
	</style>
	<style name="home_button" parent="android:style/Widget.TextView">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginLeft">20dip</item>
		<item name="android:textSize">15sp</item>
		<item name="android:textColor">@color/button_text</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_gravity">center</item>
		<item name="android:background">@drawable/home_button</item>
	</style>
	<style name="mydialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
 <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
 <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
 <!-- 无标题 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
 <!-- 模糊 -->
        <item name="android:textColor">#ffffff</item>
    </style>
</resources>
